{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "channel": "preview",
      "ios": {
        "simulator": false,
        "distribution": "internal"
      },
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "distribution": "store"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
